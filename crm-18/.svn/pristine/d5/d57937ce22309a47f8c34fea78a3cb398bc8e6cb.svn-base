package com.bjpowernode.crm.settings.service;

import java.util.List;

import com.bjpowernode.crm.settings.domain.DicType;

public interface DicTypeService {

	/**
	 * 根据编码获取字典类型对象
	 * @param code 编码
	 * @return null表示字典类型不存在
	 */
	DicType getByCode(String code);

	/**
	 * 保存字典类型
	 * @param dt
	 * @return
	 */
	int save(DicType dt);

	/**
	 * 查询所有的字典类型
	 * @return
	 */
	List<DicType> getAll();

}
